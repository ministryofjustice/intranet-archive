{
  "name": "intranet-archive",
  "version": "1.0.0",
  "description": "A simple application for generating snapshots of the MoJ Intranet.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run development",
    "development": "npx mix",
    "watch": "npx mix watch",
    "watch-poll": "npx mix watch -- --watch-options-poll=1000",
    "hot": "npx mix watch --hot",
    "prod": "npm run production",
    "production": "npx mix --production && rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/intranet-archive.git"
  },
  "author": "Ministry of Justice D&T",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ministryofjustice/intranet-archive/issues"
  },
  "homepage": "https://github.com/ministryofjustice/intranet-archive#readme",
  "devDependencies": {
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "browserify-zlib": "^0.2.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "express": "^4.18.2",
    "govuk-frontend": "^4.4.0",
    "helmet": "^6.0.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "request": "^2.88.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0"
  }
}
